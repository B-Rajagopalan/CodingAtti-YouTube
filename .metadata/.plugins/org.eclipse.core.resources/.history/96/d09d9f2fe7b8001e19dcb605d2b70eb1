package sqrt;

public class sqrt1 
{
	public static void main(String[] args) 
	{
		int n =40, precision =1;
		double whole = 0;
		
		for(int i=1;i<n/2;i++)
		{
			if(i*i<n)
			{ 
				whole = i;
				break;
			}
		}
		
		double number = 0.1;
		
		for(int i =1;i<=precision;i++)
		{
			whole+=number;
			
			if(whole*whole<=n)
			{
				System.out.println(whole);
			}
		}
	}
}
